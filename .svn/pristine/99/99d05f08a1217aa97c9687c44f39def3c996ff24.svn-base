.system_notice_wrap{height: 30px;padding:0 20px 0 46px;background:#333 url('../images/i_speaker.png') 20px center no-repeat;background-size: 15px;cursor: pointer;z-index: 10;}
.system_notice:after{content: '';display: block;clear: both;}
.system_notice p{height: 30px;line-height: 29px;color: #eee;font-weight: normal;width: auto!important;padding-left: 30px;}

.st_notice_popup{display: none;}
.st_notice_popup .st_notice ul{border-top: 1px solid #eee;}
.st_notice_popup .st_notice ul li{font-size: 14px;color: #333;line-height: 20px;padding: 20px 0 20px 24px;border-bottom: 1px solid #eee;background:url('../images/i_speaker.png') 0px 23px no-repeat;background-size: 15px;}

.not_scroll{position: fixed;overflow: hidden;width: 100%;height: 100%;}
.not_scroll .innopay{position: relative;top: 0;}

.terms > ul li:last-child{padding-bottom: 0;}
.terms > ul{height: auto; position: relative;}
.terms > ul.off{padding: 10px 24px;
	transition: all 0.2s ease-in-out;
  	-webkit-transition: all 0.2s ease-in-out;
  	-moz-transition: all 0.2s ease-in-out;
  	-ms-transition: all 0.2s ease-in-out;
   	-o-transition: all 0.2s ease-in-out;}
.terms > ul.off li{display: none;}
.terms > ul.off .terms_li_show{display: inline-block;}
.terms_li_show{line-height: 15px;color: #666;padding: 5px 0;font-size: 13px;float: left;width: 100%;position: relative;display: none;}
.terms_li_show:after{z-index: 1; content:"";display:block;position:absolute;right:0px;top:11px;width:14px;height:8px;background: url('../images/arrow_b_gray.png') no-repeat 0 0px;background-size:14px 8px;}

/*간편결제 선택*/
.simple_pay_list{border-radius: 3px; background: #fff;text-align: center;}
.simple_pay_list:after{content:"";display:block;clear:both;}
.simple_pay_list a{color:#666;float: left; display: inline-block;height: 50px;border-right:1px solid #f0f1f2;border-bottom:1px solid #f0f1f2;width: 33.3%;text-align:center;-webkit-appearance:none;-webkit-tap-highlight-color:transparent;}
.simple_pay_list a.on{color:#222;font-weight: 500;background-color: #fff3db;}
.simple_pay_list a p{padding-top: 20px; background-size: auto 20px;line-height: 20px; background-position: center;background-repeat: no-repeat;margin:0 auto;margin-top: 15px;text-align: center;font-size: 13px;display: inline-block;width: 100%;}
.simple_pay_list a:nth-child(3n){border-right: none;}
.simple_pay_list a:nth-last-child(1){border-bottom: none;}
.simple_pay_list a:nth-last-child(2){border-bottom: none;}
.simple_pay1{background-image: url('../../images/simple_pay1.png');}
.simple_pay2{background-image: url('../../images/simple_pay2.png');}
.simple_pay3{background-image: url('../../images/simple_pay3.png');}
.simple_pay4{background-image: url('../../images/simple_pay4.png');}
.simple_pay5{background-image: url('../../images/simple_pay5.png');}
